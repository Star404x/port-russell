<%- include("../partials/head") %>
<%- include("../partials/nav") %>

<main class="container">
  <h1>Détails réservation</h1>
  <p><strong>Catway :</strong> <%= catwayNumber %></p>
  <p><strong>Client :</strong> <%= reservation.clientName %></p>
  <p><strong>Bateau :</strong> <%= reservation.boatName %></p>
  <p><strong>Début :</strong> <%= new Date(reservation.startDate).toLocaleDateString("fr-FR") %></p>
  <p><strong>Fin :</strong> <%= new Date(reservation.endDate).toLocaleDateString("fr-FR") %></p>

  <p>
    <a class="btn" href="/catways/<%= catwayNumber %>/reservations/<%= reservation._id %>/edit">Modifier</a>
    <a class="btn" href="/catways/<%= catwayNumber %>/reservations">Retour</a>
  </p>
</main>

</body></html>
